{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-view1.html",
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "name": "default",
      "js": {"minify": false},
      "css": {"minify": true},
      "html": {"minify": false},
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "insertPrefetchLinks": true
    }
  ]
}
